#A __set local c = ofCanvas(this) \; local args = c:getArgs() \; local
xpos \, ypos \, size = args[1] \, args[2] \, args[3] \; local value
= false \; \; function M.new() \; ofWindow.addListener("mousePressed"
\, this) \; end \; \; function M.free() \; ofWindow.removeListener("mousePressed"
\, this) \; end \; \; function M.bang() \; ofPushMatrix() \; ofTranslate(xpos
\, ypos) \; ofSetColor(0) \; ofDrawRectangle(0 \, 0 \, size \, size)
\; ofSetColor(255) \; ofDrawRectangle(size / 40 \, size / 40 \, size
- size / 20 \, size - size / 20) \; ofSetColor(0) \; if value then
\; ofSetLineWidth(size / 40) \; ofDrawLine(size / 40 \, size / 40 \,
size - size / 40 \, size - size / 40) \; ofDrawLine(size - size / 40
\, size / 40 \, size / 40 \, size - size / 40) \; end \; ofPopMatrix()
\; end \; \; function M.mousePressed(e) \; if e.x > xpos and e.x <
xpos + size and e.y > ypos and e.y < ypos + size then \; value = not
value \; return value \; end \; end \; \; function M.value(float) \;
if float <= 0 then \; value = false \; else \; value = true \; end
\; return value \; end \; \;;
